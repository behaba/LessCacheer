	// Combining selectors based on selectors/details
	"Organize": {
		// Selector based combination
		"reduceSelectors": {
			"_special": "both",
			"selectors": {
				"params": [
					"#id div.class",
					"#secondary .oops",
					"#today p.boss",
					"#id div.class",
					"#today p.boss",
					"#id div.class",
					"#id div.class",
					"#secondary .oops"
				],
				"expect": [
					"#id div.class",
					"#secondary .oops",
					"#today p.boss"
				]
			},
			"details": {
				"params": [
					"test1;",
					"test2;",
					"test3;",
					"test4;",
					"test5;",
					"test6;",
					"test7;",
					"test8;"
				],
				"expect": [
					"test1;test4;test6;test7;",
					"test2;test8;",
					"test3;test5;"
				]
			}
		},

		// Detail based combination
		"reduceDetails": {
			"_special": "both",
			"selectors": {
				"params": [
					"#id div.class",
					"#secondary .oops",
					"#today p.boss",
					"#id div.class",
					"#today p.boss",
